{
  "name": "backpack.css",
  "main": "lib/backpack.css",
  "version": "2.0.0",
  "description": "A lightweight and somewhat opinionated CSS foundation that is best suited to applications ðŸŽ’.",
  "keywords": [
    "css",
    "css-library",
    "css-foundation",
    "css-reset",
    "css-normalise"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "autoprefix": "postcss lib/backpack.css --use autoprefixer --no-map --replace",
    "bundle": "postcss src/index.css --use postcss-import --no-map --output lib/backpack.css",
    "clean": "rimraf lib",
    "build": "yarn bundle && yarn autoprefix",
    "prebuild": "yarn clean",
    "version": "yarn build && git add .",
    "postversion": "git push && git push --tags && npm publish && yarn clean"
  },
  "browserslist": [
    "last 4 versions and > 0.5%",
    "Firefox ESR",
    "not ie < 11",
    "not op_mini all",
    "not dead"
  ],
  "repository": "git@github.com:chris-pearce/backpack.css.git",
  "author": "Chris Pearce <hello@cjpearce.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.4.6",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "^3.0.1",
    "normalize.css": "^8.0.1",
    "postcss-cli": "^6.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^1.16.1",
    "rimraf": "^2.6.3"
  }
}
